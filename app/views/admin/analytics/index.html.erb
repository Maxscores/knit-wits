<h1>Admin Analytics Dashboard</h1>
<p>User with the most orders: <%= @user_orders %></p>
<p>Category with highest price item:
  <%if !@categories_with_highest_price.nil? %>
      <%= @categories_with_highest_price.first.name.capitalize %>
  <% end %>
</p>

<table class="retired-items">
  <th>Item Title</th>
  <th>Current Status</th>
  <th>Times Retired</th>
  <% @retired_items.each_with_index do |item, index| %>
  <tr>
    <td><%= link_to item.title.capitalize, item_path(item) %></td>
    <td><%= item.status %></td>
    <td>><%= item.times_retired %></td>
  </tr>
  <% end %>
</table>
