<table class="cart-contents">
    <th>Order Number</th>
    <th>Status</th>
    <th>Order Placed on</th>
    <th></th>
<%@orders.each do |order| %>
  <tr id=<%= order.id %>>
    <td class="cart-item-price"> <%= link_to "#{order.id}", order_path(order) %></td>
    <td class="cart-item-price"> <%= order.status %></td>
    <td class="cart-item-price"> <%= order.created_at.strftime("%b %d, %Y")%></td>
  </tr>
<% end %>
</table>
